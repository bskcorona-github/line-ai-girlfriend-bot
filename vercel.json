{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/webhook.js": {
      "maxDuration": 10
    },
    "api/test.js": {
      "maxDuration": 5
    },
    "api/debug.js": {
      "maxDuration": 5
    }
  }
}
